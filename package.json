{
  "name": "xml-to-pdf",
  "version": "1.2.0",
  "description": "PDF generation from XML to a stream, buffer, or file",
  "main": "lib/generate.js",
  "scripts": {
    "test": "run-s test:lint test:unit test:cover test:cover-check",
    "test:lint": "eslint .",
    "test:unit": "mocha test/unit --recursive --require test/unit/helper",
    "test:cover": "istanbul cover _mocha -- -R dot test/unit --recursive --require test/unit/helper",
    "test:cover-check": "istanbul check-coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HughePaul/xml-to-pdf.git"
  },
  "author": "Paul Winkler",
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "bugs": {
    "url": "https://github.com/HughePaul/xml-to-pdf/issues"
  },
  "homepage": "https://github.com/HughePaul/xml-to-pdf",
  "dependencies": {
    "concat-stream": "^2.0.0",
    "debug": "^4.1.1",
    "deep-clone-merge": "^1.4.0",
    "hogan.js": "^3.0.2",
    "pdfkit": "^0.11.0",
    "underscore": "^1.9.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "istanbul": "^0.4.3",
    "mocha": "^7.0.1",
    "npm-run-all": "^4.0.2",
    "sinon": "^8.1.1",
    "sinon-chai": "^3.4.0"
  }
}
